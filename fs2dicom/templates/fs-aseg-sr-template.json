{
  "@schema": "https://raw.githubusercontent.com/qiicr/dcmqi/master/doc/schemas/sr-tid1500-schema.json#",

  "SeriesDescription": "Measurements",
  "SeriesNumber": "1001",
  "InstanceNumber": "1",

  "compositeContext": [
    "{{ aseg_dicom_seg_file }}"
  ],

  "imageLibrary": [{% for dcm in t1_dicom_files %}
    "{{ dcm }}",{% endfor %}
  ],

  "observerContext": {
    "ObserverType": "PERSON",
    "PersonObserverName": "Reader1"
  },

  "VerificationFlag": "VERIFIED",
  "CompletionFlag": "COMPLETE",

  "activitySession": "1",
  "timePoint": "1",

  "Measurements": [{% for measurement in aseg_dicom_metadata['segmentAttributes'][0] %}
      {
        "TrackingIdentifier": "{{ measurement.SegmentDescription }}",
        "ReferencedSegment": "{{ measurement.labelID }}",
        "SourceSeriesForImageSegmentation": "{{ t1_dicom_series_instance_uid }}",
        "segmentationSOPInstanceUID": "{{ dicom_seg_instance_uid }}",
        "Finding": { {% for k in measurement.SegmentedPropertyCategoryCodeSequence %}
          "{{ k }}": "{{ measurement.SegmentedPropertyCategoryCodeSequence[k] }}",{% endfor %}
        },
        "FindingSite": { {% for k in measurement.SegmentedPropertyTypeCodeSequence %}
          "{{ k }}": "{{ measurement.SegmentedPropertyTypeCodeSequence[k] }}",{% endfor %}
        },
        "measurementItems": [
          {
            "value": "{{ aseg_stats_data[aseg_stats_data.SegId==measurement.labelID].Volume_mm3.values[0] }}",
            "quantity": {
              "CodeValue": "G-D705",
              "CodingSchemeDesignator": "SRT",
              "CodeMeaning": "volume"
            },
            "units": {
              "CodeValue": "mm3",
              "CodingSchemeDesignator": "UCUM",
              "CodeMeaning": "cubic millimeter"
            }
          }
        ]
      },{% endfor %}
  ]
}
